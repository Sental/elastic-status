<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:DCKAP_Test:etc/cron_groups.xsd">
    <group id="rs_elastic_status">
        <schedule_generate_every>60</schedule_generate_every>
        <schedule_ahead_for>60</schedule_ahead_for>
        <schedule_lifetime>120</schedule_lifetime>
        <history_cleanup_every>120</history_cleanup_every>
        <history_success_lifetime>120</history_success_lifetime>
        <history_failure_lifetime>600</history_failure_lifetime>
        <use_separate_process>1</use_separate_process>
    </group>
</config>
